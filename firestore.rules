rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /users/{userId}{
      allow read: if request.auth.uid == userId || request.auth.uid == "tsWtvG6iSSPRaYaqv47yJYXVaG73";
      allow write: if request.auth.uid == userId || request.auth.uid == "tsWtvG6iSSPRaYaqv47yJYXVaG73";
    }
  }
}